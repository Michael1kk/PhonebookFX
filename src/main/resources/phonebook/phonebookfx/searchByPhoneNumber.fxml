<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane prefHeight="200.0" prefWidth="410.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="phonebook.phonebookfx.SearchByPhoneNumberController">
    <Label layoutX="18.0" layoutY="30.0" text="Введите номер телефона контакта">
        <font>
            <Font name="Times New Roman Bold" size="24.0" />
        </font>
    </Label>
   <Label layoutX="49.0" layoutY="58.0" text="(формат: +7XXXXXXXXXX)">
      <font>
         <Font name="Times New Roman Bold" size="24.0" />
      </font>
   </Label>
    <TextField fx:id="entry" alignment="CENTER" layoutX="55.0" layoutY="100.0" onKeyPressed="#getPhone" prefWidth="300.0">
        <font>
            <Font name="Times New Roman" size="14.0" />
        </font>
    </TextField>
</Pane>
